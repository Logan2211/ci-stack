# This file is generated by Ansible
#  DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN
#

[executor]
finger_port = {{ zuul_finger_port }}
private_key_file = {{ zuul_user_home }}/.ssh/nodepool_id_rsa
log_config = {{ zuul_file_executor_logging_conf_dest }}
workspace_root = {{ zuul_user_home }}/workspace

[fingergw]
listen_address = {{ zuul_fingergw_listen_address }}
log_config = {{ zuul_file_fingergw_logging_conf_dest }}
user = {{ zuul_user_name }}

[gearman]
port = 4730
server = 127.0.0.1

[gearman_server]
listen_address = 127.0.0.1
log_config = {{ zuul_file_gearman_logging_conf_dest }}
start = true

[merger]
git_dir = {{ zuul_user_home }}/git
git_user_name = {{ zuul_merger_git_user_name }}
git_user_email = {{ zuul_merger_git_user_email }}
log_config = {{ zuul_file_merger_logging_conf_dest }}

[scheduler]
tenant_config = {{ zuul_file_main_yaml_dest }}
log_config = {{ zuul_file_scheduler_logging_conf_dest }}
state_dir = {{ zuul_user_home }}

[web]
log_config = {{ zuul_file_web_logging_conf_dest }}
listen_address = {{ zuul_web_listen_address }}

[zookeeper]
hosts = {{ zuul_zookeeper_servers }}

{% for conn_name in zuul_connections.keys() %}
[connection {{ conn_name }}]
{%   for k,v in zuul_connections[conn_name].items()  %}
{{ k }} = {{ v }}
{%   endfor %}

{% endfor %}
